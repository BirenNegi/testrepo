                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText> select 
Contracts.SubcontractNumber as Number,Trades.Name AS TradeName,Trades.code,Variations.Header,Variations.Description
,Contracts.ApprovalDate, isNull(Variations.Type,'')as Type, isNull(CV.Number,0) as CVnumber,Variations.Amount,	Variations.Allowance,
 S.SubContractorId,S.BusinessUnitId,S.Name,isnull(S.shortname,S.Name)as Shortname

/*
--,Projects.ProjectId,Projects.Number, Projects.Name AS ProjectName
--,Contracts.ParentContractId AS ParentContractId,Contracts.ContractId,Contracts.Status,Contracts.Number,Contracts.WriteDate,Contracts.CheckQuotes,Contracts.CheckWinningQuote
--,
--Contracts.SiteInstruction,Contracts.SubcontractorReference
--,Variations.VariationId,Variations.ContractId,	Variations.TradeCode,--san

--Variations.BudgetAmountTradeInitial,Variations.Number,Variations.BudgetId,Variations.ClientVariationTradeId,Variations.CreatedDate	*/

from  Variations  
inner JOIN   Contracts ON Contracts.ContractId = Variations.ContractId and Contracts.ParentContractId is not null
inner JOIN Trades ON Contracts.TradeId = Trades.TradeId
inner JOIN Projects ON Trades.ProjectId = Projects.ProjectId
inner JOIN TradeParticipations ON Trades.TradeId=TradeParticipations.TradeId and subcontractorid is not null and Rank=1
inner JOIN  SubContractors  S ON S.SubContractorId = TradeParticipations.SubContractorId

left outer JOIN [ClientVariationsTrades] CT ON CT.ClientvariationTradeId=Variations.ClientvariationTradeId
left outer JOIN [ClientVariations] CV ON CV.ClientvariationId=CT.ClientvariationId

where Projects.ProjectId=ISNULL(@ProjectID,Projects.ProjectId) 
--and S.BusinessUnitId= ISNULL(@BusinessUnit,S.BusinessUnitId) 
--and S.SubContractorId=ISNULL(@Subcontractor,S.SubContractorId)
--and Projects.[status]=ISNULL(@Status,Projects.[status])
and Contracts.ApprovalDate is not Null
order by Trades.code,S.shortname,Projects.Name,SubcontractNumber--,Tradecode,variationid</CommandText>
