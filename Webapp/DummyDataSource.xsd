                      <Parameter AllowDbNull="false" AutogeneratedName="ProjectID" ColumnName="ProjectId" DataSourceName="SOSTest_Pro.dbo.Projects" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@ProjectID" Precision="0" ProviderType="Int" Scale="0" Size="4" SourceColumn="ProjectId" SourceColumnNullMapping="false" SourceVersion="Current" />
